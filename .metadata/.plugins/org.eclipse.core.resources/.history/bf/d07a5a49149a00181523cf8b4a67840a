
public class OrderItem {
	
	//Properties
	private int id;
	private Book book;
	private int quantity;
	
	public OrderItem(Book book1, int i) {
		// TODO Auto-generated constructor stub
		this.setBook(book1);
		this.quantity = i;
	}

	public Book getBook() {
		return book;
	}

	public void setBook(Book book) {
		this.book = book;
	}
}
