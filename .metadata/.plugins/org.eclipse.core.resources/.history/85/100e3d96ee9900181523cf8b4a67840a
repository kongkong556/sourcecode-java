
public class Employee {

	int id;
	public Employee(int id) {
		this.id = id;
	}
	public Employee() {
	}
	
	@Override
	public String toString() {
		return "Employee [id=" + id + "]";
	}
	public static void main(String[] args) {
		Employee e = new Employee();
		System.out.println(e.toString());
	}
}
