package com.pcc.th.xxx;
public class Base{
	protected void call() {
		System.out.println("Call From Base");
	}
}
class Sub extends Base {

	@Override
	protected void call() {
		// TODO Auto-generated method stub
		super.call();
		System.out.println("Call From Sub");
	}
	
}

public class Base {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Sub x = new Sub();
		x.call();
	}

}
